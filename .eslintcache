nts\\layout\\Menu.jsx",[],"C:\\react\\myapp\\src\\components\\layout\\Content.jsx",[],"C:\\react\\myapp\\src\\views\\examples\\NotFound.jsx",[],"C:\\react\\myapp\\src\\views\\examples\\Home.jsx",[],"C:\\react\\myapp\\src\\views\\examples\\UseEffect.jsx",["96","97","98","99","100"],"//Toma alguma ação assim que um determinado estado é acionado\nimport React, { useState, useEffect } from 'react'\nimport PageTitle from '../../components/layout/PageTitle'\nimport SectionTitle from '../../components/layout/SectionTitle'\n\n\nconst UseEffect = (props) => {\n\n    const [number, setNumber] = useState(1)\n    const [fatorial, setFatorial] = useState(1)\n    const [text, setText] = useState(\"\")\n\n    const [parouimpar, setParouimpar] = useState(\"\")\n\n\n    const calcularFatorial = (n) => {\n\n        Number(n)\n        if (n < 0) return -1\n        if (n == 0) return 1\n        if (number < 1111) {\n            return calcularFatorial(n - 1) * n\n        }\n\n    }\n\n    const update = (target) => {\n        let fat = calcularFatorial(target)\n        setNumber(target)\n        setFatorial(fat)\n    }\n\n    const validarParouImpar = () => {\n        if (number > -1) {\n            if (number % 2 == 0) {\n                setParouimpar(\"Par\")\n            }\n            if (number % 2 == 1) {\n                setParouimpar(\"Impar\")\n            }\n        }else{\n            setParouimpar(\"Numeros Positivos Plis!\")\n        }\n    }\n    const validarMensagem = () => {\n        if (fatorial > 1000000) {\n            setText(\"Ao Infinito\")\n\n        }\n        else {\n            setText(\"Bons Calculos\")\n        }\n    }\n\n    useEffect(() => {\n        validarParouImpar()\n    }, [number])\n\n    useEffect(() => {\n        validarMensagem()\n\n    }, [fatorial])\n\n\n    return (\n        <div className=\"UseEffect\">\n            <PageTitle\n                title=\"Hook UseEffect\"\n                subtitle=\"Permite executar efeitos colaterais em componentes funcionais!\"\n            />\n            <div>\n                <span className=\"text\">Fatorial: </span>\n                <span className=\"text red\">{fatorial}</span>\n            </div>\n            <div className=\"cen\"></div>\n            <input type=\"number\" className=\"input\"\n                value={number}\n                onChange={e => update(e.target.value)}\n            />\n\n            <span className=\"text red\">{text}</span>\n\n            <SectionTitle\n                title=\"Second #02\"\n            />\n\n            <span className=\"text green\">{parouimpar}</span>\n        </div>\n    )\n}\n\nexport default UseEffect\n","C:\\react\\myapp\\src\\views\\examples\\UseCallback.jsx",[],"C:\\react\\myapp\\src\\views\\examples\\UseMemo.jsx",["101"],"//Junção do State com Effect\nimport React, { useEffect, useState,useMemo } from 'react'\nimport PageTitle from '../../components/layout/PageTitle'\n\nfunction sum(a, b) {\n    const future = Date.now() + 1000\n    while (Date.now() < future) { } //...wait\n    return a + b\n}\n\nconst UseMemo = (props) => {\n\n    const [n1, setN1] = useState(0)\n    const [n2, setN2] = useState(0)\n    const [n3, setN3] = useState(0)\n\n    const result = useMemo(() => sum(n1, n2), [n1,n2])\n\n    // const[result,setResult] = useState(0)\n    // useEffect(()=>{\n    //     setResult(sum(n1,n2))\n    // },[n1,n2])\n    //const result =  sum(n1,n2)   \n    return (\n        <div className=\"UseMemo\">\n            <PageTitle\n                title=\"Hook UseMemo\"\n                subtitle=\"Retorna um valor memoizado!\"\n            />\n\n            <div className=\"center\">\n                <input type=\"text\" className=\"input\"\n                    value={n1}\n                    onChange={e => setN1(parseInt(e.target.value))}\n                />\n                <input type=\"text\" className=\"input\"\n                    value={n2}\n                    onChange={e => setN2(parseInt(e.target.value))}\n                />\n                <input type=\"text\" className=\"input\"\n                    value={n3}\n                    onChange={e => setN3(parseInt(e.target.value))}\n                />\n                <span className=\"text\">{result}</span>\n            </div>\n        </div>\n    )\n}\n\nexport default UseMemo\n","C:\\react\\myapp\\src\\views\\examples\\UseRef.jsx",[],"C:\\react\\myapp\\src\\views\\examples\\UseState.jsx",[],"C:\\react\\myapp\\src\\views\\examples\\UseContext.jsx",[],"C:\\react\\myapp\\src\\views\\examples\\UseReducer.jsx",[],"C:\\react\\myapp\\src\\views\\examples\\UseCustom.jsx",[],"C:\\react\\myapp\\src\\components\\layout\\PageTitle.jsx",[],"C:\\react\\myapp\\src\\components\\layout\\SectionTitle.jsx",[],"C:\\react\\myapp\\src\\views\\examples\\UseCallBackButtons.jsx",[],"C:\\react\\myapp\\src\\data\\DataContext.js",[],"C:\\react\\myapp\\src\\data\\Store.jsx",[],"C:\\react\\myapp\\src\\store\\config.js",[],"C:\\react\\myapp\\src\\hooks\\useCounter.js",[],"C:\\react\\myapp\\src\\hooks\\useFetch.js",["102"],"\r\nimport React, { useState, useEffect } from 'react'\r\n\r\nexport const useFetch = (url, method = 'get') => {\r\n\r\n    const [response, setResponse] = useState({\r\n        data: null,\r\n        loading: true,\r\n    })\r\n\r\n    useEffect(() => {\r\n        fetch(url,{method})\r\n        .then(resp=>resp.json())\r\n        .then(json=>setResponse({\r\n            data: json,\r\n            loading: false\r\n        }))\r\n    }, [url, method])\r\n\r\n    return response\r\n}\r\n",{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","severity":1,"message":"108","line":20,"column":15,"nodeType":"109","messageId":"110","endLine":20,"endColumn":17},{"ruleId":"107","severity":1,"message":"108","line":35,"column":28,"nodeType":"109","messageId":"110","endLine":35,"endColumn":30},{"ruleId":"107","severity":1,"message":"108","line":38,"column":28,"nodeType":"109","messageId":"110","endLine":